{
  "name": "summaly-lambda",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "node ./scripts/post-install.js",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/serve.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "DISABLED_HOSTNAMES=[\\\"example.org\\\"] vitest",
    "deploy": "pnpm run build && zip -FS -r function.zip built package.json && aws lambda update-function-code --function-name summaly-lambda --zip-file fileb://function.zip --architectures arm64 --no-cli-pager"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.5",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@tsconfig/strictest": "^2.0.6",
    "@types/aws-lambda": "^8.10.156",
    "@types/node": "^22.18.11",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@googleapis/youtube": "^28.0.0",
    "@hono/zod-validator": "^0.7.4",
    "@misskey-dev/summaly": "^5.2.4",
    "cheerio": "1.1.2",
    "debug": "^4.4.3",
    "hono": "^4.10.1",
    "html-entities": "^2.6.0",
    "secure-json-parse": "^4.1.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4"
