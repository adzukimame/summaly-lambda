{
  "name": "summaly-lambda",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "node ./scripts/post-install.js",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/serve.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "DISABLED_HOSTNAMES=[\\\"example.org\\\"] vitest",
    "deploy": "pnpm run build && zip -FS -r function.zip built package.json && aws lambda update-function-code --function-name summaly-lambda --zip-file fileb://function.zip --architectures arm64 --no-cli-pager"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.2",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.16.11",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "@googleapis/youtube": "^20.0.0",
    "@misskey-dev/summaly": "^5.1.0",
    "cheerio": "1.0.0",
    "debug": "^4.3.7",
    "hono": "^4.6.4",
    "html-entities": "^2.5.2"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276"
}
